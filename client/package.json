{
  "name": "music_app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "root": "./src",
  "source": "./src/index.html",
  "targets": {
    "default": {
      "distDir": "./dist",
      "context": "service-worker"
    }
  },
  "scripts": {
    "build-manifest": "parcel build --no-content-hash --target ./src/workers/sw.ts && workbox injectManifest",
    "app-watch": "webpack serve --mode development --config ./webpack.config.js && env-cmd -f",
    "app-build": "webpack build && env-cmd -f",
    "start-server": "npm --prefix ../server run start:debug",
    "dev": "concurrently --kill-others \"npm run start-server\" \"npm run app-watch\"",
    "dev-build": "concurrently --kill-others \"npm run start-server\" \"npm run app-build-watch\"",
    "build": "parcel build -d dist/ ./public/index.html --detailed-report --log-level info && workbox injectManifest",
    "start": "npm run build && node ./server/server.js"
  },
  "author": "",
  "license": "ISC",
