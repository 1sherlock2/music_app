{
  "extends": [
    "@parcel/config-default",
    "@parcel/config-webextension"
  ],
  "resolvers": [
    "@parcel/resolver-glob",
    "..."
  ],
  "transformers": {
    "*.{ts,tsx}": [
      "@parcel/transformer-typescript-tsc"
    ],
    "*.png": [
      "@parcel/transformer-raw"
    ]
  },
  "optimizers": {
    "*.{jpg,jpeg,png}": []
  },
  "reporters": [
    "parcel-reporter-bundle-manifest"
  ]
}